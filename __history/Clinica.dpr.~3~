program Clinica;

uses
  Vcl.Forms,
  uClinica in 'uClinica.pas' {Form1},
  uCadPaciente in 'uCadPaciente.pas' {Form2},
  uCadAgendamento in 'uCadAgendamento.pas' {Form3};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TForm3, Form3);
  Application.Run;
end.
